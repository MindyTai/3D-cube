{
  "extends": ["airbnb", "prettier", "plugin:es5/no-es2016"],
  "env": {
    "browser": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 5,
    "sourceType": "module"
  },
  "plugins": [
    "prettier",
    "es5"
  ],
  "rules": {
    "prefer-template": "off",
    "prefer-destructuring": "off",
    "func-names": "off",
    "no-var": "off",
    "vars-on-top": "off",
    "no-console": "warn",
    "no-underscore-dangle": "off",
    "no-unused-vars": "warn",
    "no-continue": "off",
    "no-bitwise": "off",
    "consistent-return": "off",
    "class-methods-use-this": "off",
    "prettier/prettier": ["error", {
      "singleQuote": true,
      "trailingComma": "none",
      "arrowParens": "always"
    }]
  }
}